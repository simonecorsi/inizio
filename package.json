        "@semantic-release/git": "^10.0.0",
        "@semantic-release/github": "^9.0.3",
        "@semantic-release/npm": "^10.0.3",
        "@semantic-release/release-notes-generator": "^12.1.0",
        "c8": "^9.1.0",
        "eslint": "^8.31.0",
